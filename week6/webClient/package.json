{
  "name": "simple-ng2-webpack",
  "version": "0.0.1",
  "description": "A webpack client for a simple angular2 application",
  "repository": {
    "type": "git",
    "url": "https://github.com/npm/npm.git"
  },
  "scripts": {
    "start": "webpack-dev-server --config config/webpack-dev-local.js --inline --progress --port 8080",
    "test": "karma start",
    "prod": "npm run clean:dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "build": "npm run clean:dist && webpack --config config/webpack-dev-server.js --progress --profile --bail",
    "clean:dist": "node ./config/clean-dist.js"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "~5.2.11",
    "@angular/cdk": "~5.2.5",
    "@angular/common": "~5.2.11",
    "@angular/compiler": "5.2.11",
    "@angular/core": "~5.2.11",
    "@angular/forms": "~5.2.11",
    "@angular/http": "~5.2.11",
    "@angular/material": "~5.2.5",
    "@angular/platform-browser": "~5.2.11",
    "@angular/platform-browser-dynamic": "~5.2.11",
    "@angular/router": "~5.2.11",
    "angular2-highcharts": "^0.5.5",
    "core-js": "^2.4.1",
    "highcharts": "^6.1.0",
    "highcharts-3d": "^0.1.7",
    "highcharts-more": "^0.1.7",
    "moment": "~2.22.1",
    "rxjs": "^5.5.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@types/jasmine": "~2.8.0",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^6.0.45",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^3.0.4",
    "codelyzer": "^4.2.1",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "2.0.0-beta.5",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.16.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.1",
    "node-sass": "^4.5.3",
    "null-loader": "^0.1.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.13.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.4.3",
    "typescript": "~2.4.2",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.1",
    "webpack-merge": "^3.0.0"
  }
}
